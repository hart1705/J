@model PORTAL.WEB.Models.ApplicationUserViewModels.ApplicationUserModel
@{ Html.RenderPartial("_FormBehavior", Model.FormBehavior); }
<form asp-controller="ApplicationUser" asp-action="Update"
      id="record-form"
      data-ajax-mode="replace"
      data-ajax-update="#record-form-container"
      data-ajax-loading="#application_loading"
      data-ajax="true"
      data-ajax-complete="Portal.Exec_FormBehavior"
      data-ajax-method="POST"
      class="form-horizontal">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="FormType" />
    <div class="form-group form-group-sm">
        <label asp-for="UserName" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="UserName" readonly class="form-control" />
            <span asp-validation-for="UserName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="RegistrationType" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="RegistrationType" readonly class="form-control" />
            <span asp-validation-for="RegistrationType" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="FirstName" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="FirstName" class="form-control" />
            <span asp-validation-for="FirstName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="MiddleName" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="MiddleName" class="form-control" />
            <span asp-validation-for="MiddleName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="LastName" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="LastName" class="form-control" />
            <span asp-validation-for="LastName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="Email" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="Email" class="form-control" />
            <span asp-validation-for="Email" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="PhoneNumber" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="PhoneNumber" class="form-control" />
            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="Password" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="Password" class="form-control" />
            <span asp-validation-for="Password" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group form-group-sm">
        <label asp-for="ConfirmPassword" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="ConfirmPassword" class="form-control" />
            <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
        </div>
    </div>
    @*<div class="form-group form-group-sm">
            <label asp-for="SubsciptionPlanId" class="control-label col-md-3 "></label>
            <div class="col-md-9">
                <lookup input-id="SubsciptionPlanId"
                        lookup-id="lookup-subscriptionplan"
                        record-id="@Model.SubsciptionPlanId"
                        entity-name="SubsciptionPlan"
                        display-column="SubsciptionPlan_Id"
                        display-columnset="SubsciptionPlan_Id:[Subscription Plan Name]#ModifiedOn:[Modified On]"
                        input-size="sm"
                        target-modal="global-lookup-modal"
                        lookup-title="Select Subcription Plan">
                </lookup>
                <span asp-validation-for="SubsciptionPlanId" class="text-danger"></span>
            </div>
        </div>*@
</form>
<form asp-controller="ApplicationUser" asp-action="ChangeStatus"
      id="changestatus-form"
      data-ajax-mode="replace"
      data-ajax-update="#record-form-container"
      data-ajax-loading="#application_loading"
      data-ajax="true"
      data-ajax-complete="Portal.Exec_FormBehavior"
      data-ajax-method="POST"
      class="form-horizontal">
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="FormType" />
</form>